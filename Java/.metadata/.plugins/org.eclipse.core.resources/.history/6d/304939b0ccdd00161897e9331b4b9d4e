package eqsimple;

public class Rationnel{

	private int a;
	private int b;
	
	/* Setters */
	public void setA(int nb)
	{
		this.a = nb;
	}
	
	public void setB(int nb)
	{
		this.b = nb;
	}
	
	/* Getters */
	public int getA()
	{
		return this.a:
	}
	
	public int getB()
	{
		return this.b;
	}
	
	

	

}
