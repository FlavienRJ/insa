opts="${opts}"

depend() {
	need net
	after mysql
}

start() {
	ebegin "Starting Daemon FTP printer - Thanh Luu"
	#start-stop-daemon --start --exec /home/user/insa/4ASTI/linux_admin/test -- -f -d 
	eend $?
}

stop() {
	#ebegin "Stoping Daemon FTP printer - Thanh Luu"
	#kill -s USR1 /var/lib/q3ftpdaemon/daemon.pid
	#eend $?
	ebegin "Stoping Daemon FTP printer - Thanh Luu"
	kill -9 `cat /var/lib/printserver/printserver.pid`
	eend $?
}

